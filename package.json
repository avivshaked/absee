{
  "name": "absee",
  "version": "0.1.0",
  "description": "A library for constructing ab-tests",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "ava",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js"
